<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="568.0" prefWidth="770.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wareHouse.UserController">
    <stylesheets>
        <URL value="@user.css" />
    </stylesheets>
    <children>
        <ImageView fitHeight="571.0" fitWidth="770.0" layoutX="-1.0" layoutY="-1.0">
            <image>
                <Image url="@../Joie_%20Blue_%20And%20yes,%20various%20shades%20of%20it_.jpg" />
            </image>
        </ImageView>
        <TextField fx:id="txtSearch" alignment="CENTER_RIGHT" layoutX="25.0" layoutY="119.0" onKeyReleased="#searchUser" prefHeight="39.0" prefWidth="458.0" promptText="محرر البحث" styleClass="user_field" stylesheets="@user.css" />
        <Label fx:id="user_mangment" alignment="CENTER" layoutX="236.0" layoutY="34.0" prefHeight="35.0" prefWidth="262.0" styleClass="header-title" stylesheets="@user.css" text="إدارة المستخدمين" textAlignment="CENTER" textFill="#fcfafa">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Pane layoutX="510.0" layoutY="175.0" prefHeight="375.0" prefWidth="246.0" styleClass="form-card" stylesheets="@supplier.css">
            <children>
                <Button fx:id="add" layoutX="130.0" layoutY="260.0" mnemonicParsing="false" onAction="#addUser" prefHeight="41.0" prefWidth="105.0" styleClass="btn-add" stylesheets="@supplier.css" text="إضافة">
                    <effect>
                        <InnerShadow />
                    </effect>
                </Button>
                <Button fx:id="update" alignment="CENTER" layoutX="14.0" layoutY="260.0" mnemonicParsing="false" onAction="#editUser" prefHeight="41.0" prefWidth="105.0" styleClass="btn-update" stylesheets="@supplier.css" text="تعديل">
                    <effect>
                        <InnerShadow />
                    </effect>
                </Button>
                <Button fx:id="delete" layoutX="130.0" layoutY="320.0" mnemonicParsing="false" onAction="#deleteUser" prefHeight="42.0" prefWidth="105.0" styleClass="btn-delete" stylesheets="@supplier.css" text="حذف">
                    <effect>
                        <DropShadow />
                    </effect>
                </Button>
                <Button fx:id="refresh" layoutX="14.0" layoutY="320.0" mnemonicParsing="false" onAction="#refreshUser" prefHeight="41.0" prefWidth="105.0" styleClass="btn-refresh" stylesheets="@supplier.css" text="تجديد">
                    <effect>
                        <InnerShadow />
                    </effect>
                </Button>
                <TextField fx:id="txtUsername" alignment="CENTER_RIGHT" layoutX="46.0" layoutY="35.0" onKeyReleased="#Username" promptText="اسم المستخدم" styleClass="user_field" stylesheets="@user.css" />
                <TextField fx:id="txtPassword" alignment="CENTER_RIGHT" layoutX="46.0" layoutY="105.0" onKeyReleased="#Password" promptText="كلمة السر" styleClass="user_field" stylesheets="@user.css" />
                <Label fx:id="lbRole" layoutX="46.0" layoutY="150.0" prefHeight="20.0" prefWidth="182.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label fx:id="lbPass" layoutX="46.0" layoutY="80.0" prefHeight="20.0" prefWidth="182.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label fx:id="lbname" layoutX="46.0" layoutY="10.0" prefHeight="20.0" prefWidth="182.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <ComboBox fx:id="comboRole" layoutX="46.0" layoutY="175.0" onAction="#comboRole" prefHeight="47.0" prefWidth="165.0" promptText="الصلاحيه" stylesheets="@user.css">
                    <styleClass>
                        <String fx:value="combo-box-popup" />
                        <String fx:value="user_field" />
                    </styleClass>
                </ComboBox>
            </children>
        </Pane>
        <TableView fx:id="tableUsers" layoutX="14.0" layoutY="173.0" prefHeight="383.0" prefWidth="474.0" stylesheets="@user.css">
            <columns>
                <TableColumn fx:id="colRole" prefWidth="131.0" style="-fx-alignment: center;" text="الصلاحية" />
                <TableColumn fx:id="colPassword" prefWidth="120.0" style="-fx-alignment: center;" text="كلمة السر" />
                <TableColumn fx:id="colUsername" prefWidth="122.0" style="-fx-alignment: center;" text="اسم المستخدم" />
                <TableColumn fx:id="colId" prefWidth="98.0" text="رقم المستخدم" />
            </columns>
            <effect>
                <InnerShadow />
            </effect>
        </TableView>
        <ImageView fitHeight="40.0" fitWidth="40.0" layoutX="42.0" layoutY="34.0" onMouseClicked="#goBack" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../backward.png" />
            </image>
            <effect>
                <Bloom threshold="0.5" />
            </effect>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </ImageView>
    </children>
</AnchorPane>
