<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="521.0" prefWidth="846.0" styleClass="root" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wareHouse.SupplierController">
    <stylesheets>
        <URL value="@supplier.css" />
    </stylesheets>
    <children>
        <ImageView fitHeight="525.0" fitWidth="863.0" layoutX="-5.0">
            <image>
                <Image url="@../Joie_%20Blue_%20And%20yes,%20various%20shades%20of%20it_.jpg" />
            </image>
        </ImageView>
        <TextField fx:id="txtSearch" alignment="CENTER_RIGHT" layoutX="32.0" layoutY="109.0" onKeyReleased="#searchSupplier" prefHeight="39.0" prefWidth="532.0" promptText="محرر البحث" styleClass="supplier_field" stylesheets="@supplier.css" />
        <Label fx:id="sm" alignment="CENTER" layoutX="315.0" layoutY="30.0" prefHeight="35.0" prefWidth="211.0" styleClass="header-title" stylesheets="@supplier.css" text="إدارة الموردين" textAlignment="CENTER" textFill="#fcfafa">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Pane layoutX="588.0" layoutY="107.0" prefHeight="407.0" prefWidth="256.0" styleClass="form-card" stylesheets="@supplier.css">
            <children>
                <Button fx:id="add" layoutX="135.0" layoutY="297.0" mnemonicParsing="false" onAction="#addSupplier" prefHeight="41.0" prefWidth="105.0" styleClass="btn-add" stylesheets="@supplier.css" text="إضافة">
                    <effect>
                        <InnerShadow />
                    </effect>
                </Button>
                <Button fx:id="edit" alignment="CENTER" layoutX="14.0" layoutY="297.0" mnemonicParsing="false" onAction="#updateSupplier" prefHeight="41.0" prefWidth="105.0" styleClass="btn-update" stylesheets="@supplier.css" text="تعديل">
                    <effect>
                        <InnerShadow />
                    </effect>
                </Button>
                <Button fx:id="delete" layoutX="135.0" layoutY="349.0" mnemonicParsing="false" onAction="#deleteSupplier" prefHeight="41.0" prefWidth="105.0" styleClass="btn-delete" stylesheets="@supplier.css" text="حذف">
                    <effect>
                        <DropShadow />
                    </effect>
                </Button>
                <Button fx:id="refresh" layoutX="14.0" layoutY="349.0" mnemonicParsing="false" onAction="#Refresh" prefHeight="41.0" prefWidth="105.0" styleClass="btn-refresh" stylesheets="@supplier.css" text="تجديد">
                    <effect>
                        <InnerShadow />
                    </effect>
                </Button>
                <TextField fx:id="SupplierName" alignment="CENTER_RIGHT" layoutX="46.0" layoutY="35.0" onKeyReleased="#SupplierName" prefHeight="20.0" prefWidth="165.0" promptText="اسم المورد" styleClass="supplier_field" stylesheets="@supplier.css" />
                <TextField fx:id="SupplierPhone" alignment="CENTER_RIGHT" layoutX="46.0" layoutY="105.0" onKeyReleased="#SupplierPhone" prefHeight="26.0" prefWidth="165.0" promptText="تليفون" styleClass="supplier_field" stylesheets="@supplier.css" />
                <TextField fx:id="Supplieremail" alignment="CENTER_RIGHT" layoutX="46.0" layoutY="175.0" onKeyReleased="#Supplieremail" prefHeight="20.0" prefWidth="165.0" promptText="ايميل الكتروني" styleClass="supplier_field" stylesheets="@supplier.css" />
                <ComboBox fx:id="statusCombo1" layoutX="46.0" layoutY="245.0" onAction="#SupplierStatus" prefHeight="28.0" prefWidth="165.0" promptText="الحالة" styleClass="supplier_field" />
                <Label fx:id="lbStatus" layoutX="46.0" layoutY="220.0" prefHeight="17.0" prefWidth="190.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label fx:id="lbEmail" layoutX="46.0" layoutY="150.0" prefHeight="17.0" prefWidth="190.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label fx:id="lbPhone" layoutX="46.0" layoutY="80.0" prefHeight="17.0" prefWidth="190.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label fx:id="lbname" layoutX="46.0" layoutY="10.0" prefHeight="17.0" prefWidth="190.0" textFill="RED">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
            </children>
        </Pane>
        <TableView fx:id="tableSupplier" layoutX="25.0" layoutY="163.0" onMouseClicked="#loadSelected" prefHeight="348.0" prefWidth="539.0" stylesheets="@supplier.css">
            <columns>
                <TableColumn fx:id="col_status" prefWidth="85.0" text="الحالة" />
                <TableColumn fx:id="col_email" prefWidth="149.0" text="ايميل الكتروني" />
                <TableColumn fx:id="col_phone" prefWidth="119.0" text="تليفون" />
                <TableColumn fx:id="col_name" prefWidth="105.0" text="اسم المورد" />
                <TableColumn fx:id="col_id" prefWidth="77.0" text="كود المورد" />
            </columns>
            <effect>
                <InnerShadow />
            </effect>
        </TableView>
        <ImageView cache="true" fitHeight="40.0" fitWidth="40.0" layoutX="34.0" layoutY="30.0" onMouseClicked="#goBack">
            <image>
                <Image url="@../backward.png" />
            </image>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <effect>
                <Bloom threshold="0.5" />
            </effect>
        </ImageView>
    </children>
</AnchorPane>
